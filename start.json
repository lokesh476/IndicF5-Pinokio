{
  "daemon": true,
  "run": [
    {
      "method": "shell.run",
      "params": {
        "venv": "env",
        "message": [
          "source env/bin/activate",
          "python app.py"
        ],
        "on": [
          {
            "event": "/(http://[0-9.:]+)/",
            "done": true
          }
        ]
      }
    },
    {
      "method": "local.set",
      "params": {
        "url": "{{input.event[0]}}"
      }
    }
  ]
}

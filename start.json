{
  "daemon": true,
  "run": [
    {
      "method": "shell.run",
      "params": {
        "message": [
          "echo 'Activating Conda environment...'",
          "eval \"$(conda shell.bash hook)\"",
          "conda activate pinokio-env",
          "python app.py"
        ],
        "env": {
          "XDG_DATA_HOME": "./cache/HF_HOME/hub",
          "ALLOW_FILE_UPLOAD": "True",
          "COQUI_TOS_AGREED": "1"
        },
        "on": [
          {
            "event": "/(http://[0-9.:]+)/",
            "done": true
          }
        ]
      }
    },
    {
      "method": "local.set",
      "params": {
        "url": "{{input.event[0]}}"
      }
    }
  ]
}


